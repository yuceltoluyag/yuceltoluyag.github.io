{
  "name": "minel",
  "version": "1.0.0",
  "description": "Kardeşim Yılan Yap Kendini",
  "main": "gulpfile.babel.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "JEKYLL_ENV=development npx gulp",
    "start:dev": "gulp --noserviceworker",
    "build": "JEKYLL_ENV=production npx gulp build_prod",
    "deploy": "npx vercel --prod",
    "setup": "npm install && bundle install",
    "lint": "eslint '_assets/js/**/*.js'",
    "prettier": "prettier '_assets/js/**/*.js' --write",
    "lint:nofix": "eslint && npm run prettier",
    "prettier:write": "prettier '*.config.js' '*/_assets/scripts/**/*.js' --write"
  },
  "author": "yuceltoluyag",
  "license": "ISC",
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/register": "7.24.6",
    "broken-link-checker": "0.7.8",
    "browser-sync": "3.0.2",
    "canvas": "2.11.2",
    "chalk": "5.3.0",
    "del": "7.1.0",
    "dotenv": "16.4.5",
    "eslint": "9.19.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "9.0.0",
    "gulp-awspublish": "8.0.0",
    "gulp-babel": "8.0.0",
    "gulp-cssnano": "2.1.3",
    "gulp-debug": "5.0.1",
    "gulp-duration": "0.0.0",
    "gulp-gm": "0.0.9",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "9.1.0",
    "gulp-inline-source": "4.0.0",
    "gulp-less": "5.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "^6.0.0",
    "gulp-size": "5.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-terser": "2.1.0",
    "gulp-webp": "^5.0.0",
    "imagemin-webp": "^8.0.0",
    "jsdom": "24.1.1",
    "lodash.merge": "4.6.2",
    "mkdirp": "3.0.1",
    "ngrok": "4.3.3",
    "nodemon": "3.1.4",
    "postcss": "8.4.41",
    "sync-request": "6.1.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "node-sass": "^9.0.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.10.0"
  }
}
